msg = np.array([start_pt, j1_range, end_pt])

data = msg.flatten()

multi_array = MultyArrayLayout()

arr_dim = MultiArrayDimension(label="data", size=len(data), stride=1)
multi_array.dim = [arr_dim]
multi_array.data_offset = 0

arr_msg = Float64MultiArray()
arr_msg.layout = multi_array
arr_msg.data = data

publisher.publish(arr_msg)

